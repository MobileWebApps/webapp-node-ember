			<div class="container">
				<div class="row-fluid">
					<ul>
						{{#each link in footerToolbarlinks}}
							{{#if link.showLink}}
								<li data-id="{{link.linkText}}">
									{{#linkTo apps.go link.linkToRouteContext}}
